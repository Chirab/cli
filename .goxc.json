{
  "ArtifactsDest": "bin",
  "Arch": "386,amd64",
  "BuildConstraints": "windows linux darwin freebsd openbsd",
  "Resources": {
   "Include": "INSTALL*,README.md,LICENSE*",
   "Exclude": "*.go"
  },
  "PackageVersion": "1.0.0-dev",
  "ConfigVersion": "0.9",
  "OutPath": "{{.Dest}}{{.PS}}{{.Version}}{{.PS}}{{.Os}}-{{.Arch}}",
  "Env": [
    "GOPATH={{.Env.GOPATH}}{{.PS}}src{{.PS}}github.com{{.PS}}Scalingo{{.PS}}cli{{.PS}}Godeps{{.PS}}_workspace{{.PLS}}{{.Env.GOPATH}}"
  ]
}
